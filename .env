# ---- Backend selection ----
# bigquery | postgres | duckdb
MIMIC_BACKEND=bigquery

# ---- BigQuery settings ----
# Your billing project (NOT physionet-data)
WORK_PROJECT=mimic-hypercapnia
# PhysioNet hosting project (read-only)
BQ_PHYSIONET_PROJECT=physionet-data
# Dataset names exactly as shown in the Explorer
BQ_DATASET_HOSP=mimiciv_3_1_hosp
BQ_DATASET_ICU=mimiciv_3_1_icu
# Set to the ED dataset you see, e.g., mimiciv_ed or mimiciv_3_1_ed
BQ_DATASET_ED=mimiciv_ed

# ---- Postgres settings (used only if MIMIC_BACKEND=postgres) ----
MIMIC_PG_CONN_STR=postgresql+psycopg2://mimic_user:STRONG_PASSWORD@localhost:5432/mimiciv
MIMIC_SCHEMA_HOSP=mimiciv_hosp
MIMIC_SCHEMA_ICU=mimiciv_icu
MIMIC_SCHEMA_ED=mimiciv_ed

# ---- DuckDB settings (used only if MIMIC_BACKEND=duckdb) ----
MIMIC_DUCKDB_PATH=:memory: